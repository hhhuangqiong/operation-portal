{
  "name": "m800-operation-portal",
  "version": "0.1.0",
  "description": "A web portal for M800 internal server operation",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch start-server",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "start-server": "babel-node tools/startServer.js",
    "lint": "esw src --ignore-pattern src/reducers/nodesData.js",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "NODE_PATH=./src mocha tools/testSetup.js \"test/**/*.js\" --reporter progress",
    "test:cover": "NODE_PATH=./src babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"test/**/*.js\" --reporter progress",
    "test:watch": "NODE_PATH=./src npm run test -- --watch",
    "docs": "gitbook build",
    "jenkins": "npm prune && npm i --verbose && npm update",
    "prejenkins:commit": "npm run jenkins",
    "jenkins:commit": "npm run lint && npm run test:cover",
    "prejenkins:merge": "npm run jenkins",
    "jenkins:merge": "npm run build && npm run docs"
  },
  "author": "M800 Web Team",
  "license": "MIT",
  "dependencies": {
    "bem-cn": "^1.3.1",
    "express": "^4.14.0",
    "foundation-sites": "^6.2.3",
    "http-proxy-middleware": "^0.17.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "m800-web-styleguide": "^0.1.3-snapshot.103",
    "nconf": "^0.8.4",
    "object-assign": "^4.1.0",
    "pretty-data2": "^0.40.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-select": "1.0.0-beta13",
    "react-tabs": "^0.6.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.2",
    "svg-url-loader": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-preset-airbnb": "^2.0.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.8.0",
    "browser-sync": "^2.12.5",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "cheerio": "^0.20.0",
    "connect-history-api-fallback": "^1.2.0",
    "coveralls": "^2.11.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "^2.2.0",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.6.1",
    "eslint-plugin-jsx-a11y": "^1.0.4",
    "eslint-plugin-react": "^5.0.1",
    "eslint-watch": "^2.1.11",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "gitbook-cli": "^2.2.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.4.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "node-sass": "^3.7.0",
    "npm-run-all": "^1.8.0",
    "prompt": "^1.0.0",
    "react-addons-test-utils": "^15.1.0",
    "redux-immutable-state-invariant": "^1.2.3",
    "replace": "^0.3.0",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.4",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
